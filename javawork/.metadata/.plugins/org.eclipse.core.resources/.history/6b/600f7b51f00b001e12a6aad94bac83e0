package day0616;

public class OperTest_07 {

	public static void main(String[] args) {
		// 기본자료형_8개
		//1.논리형_true,false
		boolean b=false;
		
		//2.문자형
		//반드시 하나의 문자 16비트 유니코드값 표현
		//자바에서 char 문자하나 2바이트
		//기본적으로는 정수형,아스키코드
		char ch='가';
		
		//3.정수형
		byte a=-128; //1바이트 -128~127까지 가능
		short s=32000; //16bit
		int c=6700000;//32bit
		long num=100L; //64bit,뒤에 L,l을 붙혀야 long
		
		//실수형
		double d=42356.14526; //64bit,소수점
		float f=3.1234568f;//32bit
		
		//int,float는 똑같이 32bit사용하지만 기본적으로 실수형이 정수형보다 큰타입으로 처리된다
		
		//출력
		System.out.println("bool="+b);
	}

}
